<?xml version="1.0" encoding="utf-8"?>
<!-- Specials.mxml -->
<mx:TitleWindow xmlns:mx="http://www.adobe.com/2006/mxml"
        
        showCloseButton="true"
        close="titleWindow_close(event);" x="700"  y="100" width="486" height="114" 
		creationComplete="init(event);ti_panelname.setFocus();ti_panelname.drawFocus(true);" 
        dropShadowEnabled="false"
        borderAlpha="1.0"
   		backgroundImage=""
  
        layout="horizontal"
        color="#B6B9BA" xmlns:local="*"
        >

 <mx:Script> 
 	<![CDATA[
 		import mx.collections.ArrayCollection;
 			import mx.events.CloseEvent;
            import mx.managers.PopUpManager;
		 	import mx.rpc.events.*; 
		 	import mx.controls.*;
		 	import mx.events.FlexMouseEvent;
		 	  
		 	
		 	
		 	private var helpWindow:TitleWindow;

 		    private function titleWindow_close(evt:Event):void { 
                PopUpManager.removePopUp(this);
           }
            private function init(evt:Event):void {
          				
           }
          
  	            
 	]]>
 </mx:Script>
 <mx:VBox width="100%" height="100%">
 <mx:HBox width="100%">
  <mx:Label text="Name: " fontSize="15" fontWeight="bold" color="#010101"/>
  <mx:TextInput id="ti_panelname" fontSize="15" width="100%" color="#010101" fontWeight="bold" maxChars="30" />     
 </mx:HBox>
 
 <mx:HBox width="100%">
 	<mx:Spacer width="100%"/>
  	
	 <mx:Button id="btn_submit" label="Submit"  color="#010101"/>
  	<mx:Button id="btn_cancel" label="Cancel" click="titleWindow_close(event)"  color="#010101"/>
	
  	<mx:Spacer width="100%"/>
 </mx:HBox>  
 
 	
 </mx:VBox>
 
 <!--

 	-->
 

</mx:TitleWindow>
